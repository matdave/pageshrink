{
  "name": "PageShrink",
  "lowCaseName": "pageshrink",
  "description": "Automatically shrink the ouput on MODX Resources",
  "author": "matdave",
  "version": "1.0.2-pl",
  "package": {
    "elements": {
      "plugins": [
        {
          "name": "PageShrink",
          "file": "PageShrink.php",
          "description": "pageshrink.desc",
          "events": [
              "OnWebPagePrerender"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "resource_shrink",
        "type": "combo-boolean",
        "value": "1"
      },
      {
        "key": "cache_resource_shrink",
        "type": "combo-boolean",
        "value": "1"
      }
    ]
  }
}
